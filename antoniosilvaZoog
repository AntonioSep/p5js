    <title>p5js Template</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.6.0/p5.js"></script>
    <script>
      function setup() {
        createCanvas(400,400);
      }
      
      function draw() {
       background(220);
  stroke(10);
  strokeWeight(5);
  noFill();
  fill("grey");
  ellipse(100, 100, 100, 100); //left ear
  ellipse(300, 100, 100, 100); //right ear
  fill("pink");
  ellipse(100, 100, 70, 70); //inside left ear
fill("pink");
ellipse(300, 100, 70, 70); //inside right ear
fill("grey");
  ellipse(200, 200, 250, 260);//head
 fill("black");
  rect(185, 180, 30, 50); //nose
point(300, 100); //ear
point(100, 100); //ear
fill("white");
  ellipse(150, 140, 50, 50); //left eye
  ellipse(250, 140, 50, 50); // right eye
  fill("black");
  ellipse(150, 140, 30, 30); //left pupil
ellipse(250, 140, 30, 30); //right pupil
textSize(32);
text('koala', 10, 330);
fill(0, 102, 163);
text('koala', 10, 360);
fill(0, 102, 153, 51);
text('koala', 10, 390);
fill("white");
  line(300, 165, 80, 165); // glasses 
      }
    </script>
</head>

